      <Pane layoutX="61.0" layoutY="185.0" prefHeight="200.0" prefWidth="630.0" style="-fx-background-color: &quot;#D8C4B6&quot;; -fx-background-radius: 10;">
         <children>
            <Label layoutX="19.0" layoutY="13.0" text="Problem 1">
