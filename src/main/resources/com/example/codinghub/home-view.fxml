<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1280" prefWidth="2048" style="-fx-background-color: &quot;F5EFE7&quot;;" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.codinghub.controllers.HomeController">
   <children>
      <Label layoutX="61.0" layoutY="31.0" text="Welcome to Coding Hub - your place to improve the coding skills" textFill="#213555">
         <font>
            <Font name="JetBrainsMono NF ExtraBold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="125.0" text="Choose a problem to tackle today" textFill="#3E5879">
         <font>
            <Font name="Arial" size="18.0" />
